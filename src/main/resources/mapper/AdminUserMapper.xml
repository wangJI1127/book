<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wj.learn.mapper.AdminUserMapper">

    <select id="getInfo" parameterType="String" resultType="com.wj.learn.bean.AdminUserBean">
        SELECT * FROM admin_user WHERE account = #{name} AND password = #{password}
    </select>

</mapper>